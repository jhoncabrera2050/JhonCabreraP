<Window x:Class="JhonCabrera.Camiones"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JhonCabrera"
        mc:Ignorable="d"
        Title="Camiones" Height="200" Width="400">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Peso maximo: "/>
            <TextBox Name="txtNumeroDocumento"/>
            <TextBlock Text="Placa"/>
            <TextBox Name="txtPeso"/>
            <TextBlock Text="Peso vacio"/>
            <TextBox Name="txtTransportista"/>
        </StackPanel>
    </Grid>
</Window>
