<Window x:Class="JhonCabrera.Salida"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JhonCabrera"
        mc:Ignorable="d"
        Title="Salida" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Tipo de Documento:"/>
            <ComboBox Name="cmbTipoDocumento">
                <ComboBoxItem Content="DNI"/>
                <ComboBoxItem Content="Carné"/>
            </ComboBox>
            <TextBlock Text="Número de documento:"/>
            <TextBox Name="txtNumeroDocumento"/>
            <TextBlock Text="Peso:"/>
            <TextBox Name="txtPeso"/>
            <TextBlock Text="Nombre del transportista:"/>
            <TextBox Name="txtTransportista"/>
        </StackPanel>
    </Grid>
</Window>
